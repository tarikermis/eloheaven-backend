{
  "name": "eloheaven-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node -r dotenv/config dist/server.js",
    "build": "npm run clean && npm run build-ts",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon -r dotenv/config dist/server.js",
    "clean": "rimraf ./dist",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "eslint --fix . --ext .ts,.js,.tsx,.jsx",
    "pret": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "jest",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write"
    ]
  },
  "author": "fujin",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/crypto-js": "4.1.1",
    "@types/express": "4.17.13",
    "@types/html-to-text": "9.0.0",
    "@types/jest": "29.4.0",
    "@types/lodash": "4.14.184",
    "@types/mongoose-sequence": "3.0.7",
    "@types/multer": "1.4.7",
    "@types/node": "18.7.13",
    "@types/node-cron": "3.0.11",
    "@types/nodemailer": "6.4.7",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "concurrently": "7.3.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "lint-staged": "14",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "stripe": "16.0.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@types/coinbase-commerce-node": "1.0.6",
    "@types/jsdom": "21.1.0",
    "@types/random-useragent": "0.3.1",
    "@types/sharp": "0.31.1",
    "axios": "0.27.2",
    "bcrypt": "5.0.1",
    "body-parser": "1.20.0",
    "chalk": "5.0.1",
    "coinbase-commerce-node": "1.0.4",
